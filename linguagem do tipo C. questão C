#include <stdio.h>
int main() 
{
int num1, num2, divisao, subtracao, multiplicacao, adicao;
printf("valor da variavel numero 1 é:");
scanf("%d", &num1);
printf("o valor da varivel numero 2 é:");
scanf("%d", &num2);

adicao = num1+num2;
subtracao = num1-num2;
multiplicacao = num1*num2;
divisao = num1/num2;
if (num2==0)
{
   printf("error, divisao por zero");
  }
else{
   printf("o valor da divisao é:", divisao);
  }
  
  printf ("O valor da soma é: %d", adicao);
  printf ("O valor da multiplicação é: %d", multiplicacao);
  printf ("O valor da subtraçao é: %d", subtracao);
  printf ("O valor da divisão é: %d", divisao);
}
