#include <stdio.h>

int main() 
{
    float raio, altura, volume;
    const float PI = 3.14159;

    printf("Digite o raio da lata (em cm): ");
    scanf("%f", &raio);
    
    printf("Digite a altura da lata (em cm): ");
    scanf("%f", &altura);

    volume = PI * raio * raio * altura;

    printf("O volume da lata Ã©: %f \n",  volume);

    return 0;
}
